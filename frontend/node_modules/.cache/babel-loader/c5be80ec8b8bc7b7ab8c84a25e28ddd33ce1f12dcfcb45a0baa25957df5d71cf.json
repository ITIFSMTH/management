div class=\"modal-footer\">\n      <button class=\"modal__button modal__button-cancel\"\n        v-on:click=\"closeEditWorkerPassword()\">Отмена</button>\n      <button class=\"modal__button modal__button-agree\"\n        v-on:click=\"confirmEditWorkerPassword()\">Подтвердить</button>\n    </div>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'edit-worker-login-modal',\n    computed: {\n      login: {\n        get() {return this.$store.getters.editWorkerLoginModal.newLogin},\n        set(login) {this.$store.commit('updateEditWorkerLoginModalLogin', login)},\n      },\n    },\n    methods: {\n      closeEditWorkerPassword() {\n        this.$store.commit('switchEditWorkerLoginModal', false)\n      },\n      async confirmEditWorkerPassword() {\n        await this.$store.dispatch('editWorkerLogin')\n        this.$store.commit('switchEditWorkerLoginModal', false)\n      }\n    }\n  }\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}