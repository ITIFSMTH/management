ment';\n\n  export default {\n    name: \"statistic-component\",\n    data() {\n      return {\n        days: [],\n      }\n    },\n    methods: {\n      getDays() {\n        const currentDate = new Date()\n\n        this.days = Array.from({length: new Date(currentDate.getFullYear(), currentDate.getMonth(), 0).getDate()}, (_, i) => {\n          return new Date(currentDate.getFullYear(), currentDate.getMonth(), i+1)\n        })\n      },\n      getDate() {\n        return moment()\n      }\n    },\n    mounted() {\n        getDays()\n        this.$store.dispatch(\"getOperatorsStatistic\")\n    },\n}\n</script>\n\n\n<style scoped></style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}